         <xs:element minOccurs="0" ref="tei:pos"/>
         <xs:element minOccurs="0" ref="tei:subc"/>
         <xs:element minOccurs="0" ref="tei:colloc"/>
         <xs:element minOccurs="0" ref="tei:usg"/>
         <xs:element minOccurs="0" ref="tei:lbl"/>
