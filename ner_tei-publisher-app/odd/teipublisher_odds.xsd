         <xs:element ref="tei:pos"/>
         <xs:element ref="tei:subc"/>
         <xs:element ref="tei:colloc"/>
         <xs:element ref="tei:usg"/>
         <xs:element ref="tei:lbl"/>
      </xs:choice>
   </xs:group>
   <xs:group name="tei_model.lexicalRefinement_sequence">
      <xs:sequence>
         <xs:element ref="tei:gramGrp"/>
         <xs:element ref="tei:pos"/>
         <xs:element ref="tei:subc"/>
